{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    <h1>debug</h1>
    <div class="sf-toolbar-block">

        <div class="sf-toolbar-icon">
            <a href="{{ profiler_url }}?panel=dsl_platform">
                <img src="data:image/png;base64," alt="DSL Platform">
                <span>{{ collector.data|length }} reqs - {{ collector.duration }} ms</span>
            </a>
        </div>

        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-piece">
                <b>Requests</b>
                <span>{{ collector.data|length }}</span>
            </div>
            <div class="sf-toolbar-info-piece">
                <b>Total duration</b>
                <span>{{ collector.duration }} ms</span>
            </div>
        </div>

    </div>
{% endblock %}

{% block menu %}
    <span class="label">
    <span class="icon"><img src="data:image/png;base64," alt="DSL Platform"></span>
    <strong>DSL platform 213</strong>
</span>
{% endblock %}

{% block panel %}
    {% for request in collector.data %}
        <table>
            <tbody>
            <tr>
                <th>Request</th>
                <td><pre>{{ request.request }}</pre></td>
            </tr>
            <tr>
                <th>Status code</th>
                <td>{{ request.response.code }}</td>
            </tr>
            <tr>
                <th>Response</th>
                <td><pre>{{ request.response.body }}</pre></td>
            </tr>
            <tr>
                <th style="width: 25%">Duration</th>
                <td style="width: 75%">{{ request.duration }} ms</td>
            </tr>
            </tbody>
        </table>
    {% endfor %}
{% endblock %}
